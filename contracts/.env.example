# Private Key (without 0x prefix)
# IMPORTANT: Never commit your actual private key!
PRIVATE_KEY=your_private_key_here

# Treasury Address (receives platform fees)
# Leave empty to use deployer address
TREASURY_ADDRESS=

# RPC URLs
# You can use public RPCs or get dedicated ones from providers like Alchemy, Infura, QuickNode
BASE_SEPOLIA_RPC=https://sepolia.base.org
BASE_MAINNET_RPC=https://mainnet.base.org
ARBITRUM_SEPOLIA_RPC=https://sepolia-rollup.arbitrum.io/rpc
ARBITRUM_MAINNET_RPC=https://arb1.arbitrum.io/rpc

# Block Explorer API Keys (for contract verification)
# Get these from:
# - Basescan: https://basescan.org/myapikey
# - Arbiscan: https://arbiscan.io/myapikey
BASESCAN_API_KEY=your_basescan_api_key_here
ARBISCAN_API_KEY=your_arbiscan_api_key_here

# Optional: Gas Reporter
REPORT_GAS=false
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here
